/*COLORS*/
.page-404 .mark-404 {
  margin: 0 auto;
  padding: 40px 0;
  width: 50%; }
  .page-404 .mark-404 img {
    max-width: 100%; }
.page-404 .content-404 {
  width: 50%;
  margin: 0 auto;
  text-align: center; }
  .page-404 .content-404 span {
    font-family: 'verdana';
    font-size: 23px; }
  .page-404 .content-404 h4 {
    letter-spacing: 5px;
    line-height: 51px;
    margin: 23px 0; }
.page-404 .search {
  width: 33.33333333333%;
  margin: 0 auto;
  padding: 15px 0; }
.page-404 form {
  margin: 0 auto; }
  .page-404 form .inner-button {
    border: 1px solid #676460;
    height: 50px;
    width: 100%; }
    .page-404 form .inner-button input {
      border: 0;
      font-family: "Istok Web";
      width: 84%; }
    .page-404 form .inner-button span {
      border: 0;
      display: table-cell;
      width: 16%;
      vertical-align: middle; }
      .page-404 form .inner-button span button.btn {
        border: 0; }
        .page-404 form .inner-button span button.btn i {
          color: #a5a5a5; }

@media (max-width: 991px) {
  .content-404 span {
    font-size: 18px; } }
@media (max-width: 767px) {
  .content-404 span {
    font-size: 11px; } }

