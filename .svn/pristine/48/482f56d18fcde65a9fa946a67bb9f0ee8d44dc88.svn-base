    top: -2px;
    transform: rotate(180deg);
    transition-delay: 0.3s;
    z-index: 5; }
  .ed-checkbox input[type="checkbox"] {
    visibility: hidden;
    position: absolute; }
  .ed-checkbox label {
    cursor: pointer;
    padding-left: 30px; }
