/*
Checkbox
*/
.ed-checkbox {
  position: relative; }
  .ed-checkbox > span.inc {
    animation: 0.3s ease 0s normal none 1 running growCircle; }
  .ed-checkbox > .check {
    -moz-border-top-colors: none;
    -moz-border-right-colors: none;
    -moz-border-bottom-colors: none;
    -moz-border-left-colors: none;
