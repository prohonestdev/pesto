@media (min-width: 992px) {
  .header-main-nav .dropdown .dropdown-menu {
    opacity: 0;
  }

  .header-main-nav .dropdown:hover .dropdown-menu {
    display: block;
    opacity: 1;
  }

  #header .collapse {
    display: block;
  }
}
@media (max-width: 1170px) {
  .navbar-right {
    margin-right: -18px;
  }
  .navbar-right .dropdown a {
    padding-left: 17px;
    padding-right: 18px;
  }
  .navbar-right .dropdown-menu a {
    margin: 0;
  }

  .navbar-left li a {
    margin-right: 35px;
  }

  .header-center .header-main-nav .nav li a {
    padding-left: 20px;
    podding-right: 20px;
  }
}
@media (max-width: 992px) {
  .caret {
    display: inline-block;
  }

  .btn.header-btn-collapse-nav {
    float: right;
    display: block;
  }

  .btn.header-btn-collapse-top {
    display: inline-block;
  }

  .header-fixed-menu {
    background: white;
  }

  .header-top-links .navbar-left {
    background-color: white;
    position: absolute;
    top: 30px;
  }

  .header-top-links .navbar-left {
    border-radius: 5px;
    display: none;
    height: auto;
    margin: 0;
    position: absolute;
    z-index: 1000;
    -webkit-transition: all 2s ease-in;
    -moz-transition: all 2s ease-in;
    transition: all 2s ease-in;
  }
  .header-top-links .navbar-left:before {
    content: "";
    display: block;
    height: 30px;
    top: -20px;
    position: relative;
  }
  .header-top-links .navbar-left li {
    float: none;
    display: block;
  }
  .header-top-links .navbar-left li a {
    margin: 0px;
    padding: 0;
  }

  #main-nav:before {
    content: "";
    display: block;
    height: 30px;
    position: relative;
    top: -20px;
  }

  #main-nav .navbar-nav li {
    border-bottom: solid 1px;
    border-bottom-left-radius: 5px;
    border-bottom-right-radius: 5px;
    display: block;
    text-align: left;
  }
  #main-nav .navbar-nav li a {
    text-decoration: none;
  }

  #main-nav nav {
    text-align: left;
  }
  #main-nav nav ul {
    width: 100%;
  }

  .navbar-nav li a {
    padding-top: 5px;
    padding-bottom: 3px;
  }

  .header-main-nav {
    display: none;
  }

  .header-top-links .container-fluid {
    padding-left: 8px;
    padding-right: 8px;
  }
  .header-top-links .container-fluid li a {
    padding-left: 17px;
    padding-right: 17px;
  }

  .header-table .container-fluid {
    padding-left: 25px;
    padding-right: 25px;
  }

  .navbar li.dropdown-table .dropdown-menu {
    position: relative;
    margin-top: 0;
  }
  .navbar li.dropdown-table .dropdown-menu:before {
    content: "";
    height: 0;
    top: 0;
    position: relative;
  }
  .navbar li.dropdown-table .dropdown-menu .col-5-1 {
    float: left;
    width: 100%;
  }
}
@media (max-width: 768px) {
  .navbar-right {
    margin-right: -20px;
    float: right;
  }
  .navbar-right li {
    display: inline-block;
  }
  .navbar-right li a {
    padding-left: 20px;
    padding-right: 20px;
  }
  .navbar-right li ul li {
    display: block;
  }

  .navbar-right .dropdown a {
    padding-left: 12px;
    padding-right: 13px;
    padding: 0;
  }
  .navbar-right .dropdown-menu a {
    margin: 0;
  }
