/*COLORS*/
#contactForm input {
  font-family: "Roboto Condensed";
  font-size: 14px; }
#contactForm input[type="submit"] {
  border-color: #444645; }
#contactForm label {
  font-family: "Istok Web";
  font-size: 12px;
  font-weight: normal;
  text-transform: uppercase; }

.content-footer a {
  font-size: 35px; }
.content-footer h4 {
  font-family: "Roboto Condensed";
  font-size: 18px;
  margin-top: 20px;
  margin-bottom: 15px; }
.content-footer .location > div, .content-footer .details > div, .content-footer .phone > div {
  font-family: "Verdana";
  font-size: 14px;
  padding: 5px 0; }

.contactus.type1 .content-footer, .contactus.type2 .content-footer {
  padding-bottom: 30px; }

.contactus.type3 .content-footer {
  padding-bottom: 45px; }

.contactus.type1 .content-footer {
  padding-top: 90px; }

.contactus.type2 #contactForm input, .contactus.type3 #contactForm input {
  height: 43px;
  margin-bottom: 25px; }
.contactus.type2 .content-main .review-customer h3, .contactus.type3 .content-main .review-customer h3 {
  margin-bottom: 20px; }
.contactus.type2 .content-main .review-customer textarea, .contactus.type3 .content-main .review-customer textarea {
  height: 229px; }

.contactus.type2 .content-main .review-customer input[type="submit"] {
  margin-top: 16px; }
.contactus.type2 .content-footer {
  padding-top: 25px; }

.contactus.type3 .content-footer {
  padding-top: 100px; }

.contactus.type3 .content-main .review-customer {
  padding-bottom: 45px; }
  .contactus.type3 .content-main .review-customer input[type="submit"] {
    margin-top: 23px; }

