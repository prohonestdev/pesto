//GridLayout

/* ---- isotope ---- */

.grid {
    background: #FFF;
}

/* clear fix */
.grid {
    @include clearfix;
}

.grid-item {
    float: left;
}

.grid-item img {
    display: block;
    height: 100%; 
    width: 100%;
} 

.visible-opened {
    display: block;   
}
    
.visible-collapsed {
    display: none;
}

.collapsed {
    .visible-opened {
        display: none;   
    }    
    .visible-collapsed {
        display: block;
    }
}

a.toggle:hover {
	.visible-opened {
		display: none;   
	}
		
	.visible-collapsed {
		display: block;
	}

	&.collapsed {
		.visible-opened {
			display: block;   
		}    
		.visible-collapsed {
			display: none;
		}
	}
}
