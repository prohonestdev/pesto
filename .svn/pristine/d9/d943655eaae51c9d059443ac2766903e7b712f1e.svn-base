	        .links {
	        	margin-top: 12px;
                a {
                    font-size: 13.01px;
                    letter-spacing: 0.6px;
                    padding: 13.5px 15px;
                }
	        }
	        h2 {
	            color: $color28;
	        }
	    }
	    .grid-item {
	        padding: 5px;
	    }
	    .grid-width {
	        width: 25%;
	    }
	    .grid-width2 {
	        width: 50%;
	    }
	}       
	.products-container {
        margin-top: 40px;
	}
	.products {
		padding-top: 4px;
	}
}
.portfolio-item {
	.explanation {
		.title {
			margin-bottom: 12px;
	        margin-top: 21px;
		}
		h1.small {
			margin-bottom: 15px;
			margin-top: 0px;
		}
	}
	.images {
		.image {
			padding: 20px 0;
		}
	}
    .over-comment {
        h3 {
            margin-bottom: 12px;
            margin-top: 16px;
            
        }
    }
	.pagination {
		margin-top: 20px;
	}
	.pagination-image {
		margin-top: 100px;
        padding-bottom: 40px;
	}
	.products-top {
		.pull-right
		,.pull-left {
			li {
				padding: 0;
			}
			a {
				font-size: 16px;
			}
		}
		.pull-left {
			margin-left: -28px;
		}
		.pull-right {
			margin-right: -28px;
			a {
				display: block;
				padding: 0 27px;
			}
		}
        .filter-container {
            .over-toggle {
                color: $color24;
                font: normal 16px/1 $font-primary;
                margin-right: -7px;
                padding-top: 2px;
                i {
                    font-size: 11px;
                    line-height: 16px;    
                }
            }
        }
	}
	.sidebar {
		margin-top: 29px;
        	
		.content
		,.fans {
			padding: 14px 0;
		}
		
		.title {
			color: $color-dark;
			font: 14px $font-secondary;
            letter-spacing: 1.5px;
			margin-top: 0;
            padding: 12px 0;
		}
		
		.content {
			color: #dadada !important;
			font: 14px $font-tertiary;
		}
		ul.share {
			margin-top: 4px;
		}	
	}
	&.type1 {
        .sidebar {
            margin-top: 38px;
        }
        .category {
            h2 {
                margin-top: 30px;
                padding: 0;
            }
        }
        .date {
            h2 {
                margin-top: 27px;   
                padding: 0;
            }
            .content {
                display: block;
                margin-top: 13px;
                padding: 0;
            }
        }
        .fans {
            margin-bottom: 17px;
            padding: 0;
        }
        .explanation {
            p {
                color: $color54;
            }
            .quote {
                color: $color14;
            }
        }
        .images {
            padding-top: 50px;
        }
		.project {
			margin-top: 40px;
		}
		.share li {
			margin-right: 10px;
		}
        .share {
            h2 {
                margin-top: 16px;
                margin-bottom: 4px;
            }
        }
	}
	&.type2 {
		.project {
			margin-top: -6px;
            margin-bottom: 27px;
            .explanation p {
                color: $color14;
            }
			.share{
				margin-left: 3px;
				.title {
					color: $color-dark;
					font: 14px $font-secondary;
                    letter-spacing: 1px;
                    margin-left: -2px;
					padding: 8px 0px;
				}
				ul {
					margin-top: 22px;
					li {
						margin-right: 10px;
					}
				}
				>* {
					float: left;
					margin-right: 13px;
				}
			}
		}
		.sidebar {
			.category {
				margin-top: -6px;
			}
			.fans {
				padding-top: 13px;
			}
		}
	}
	&.type3 {
		.products-top {
			margin-bottom: 5px;
		} 
		.pagination-container {
			padding-bottom: 37px;
			padding-top: 5px;
			.title
			,.pagination {
				margin-top: 20px;
			}
		}
		.products-container {
			padding-top: 46px;
		}
		.product {
			@include clearfix;
			padding: 27px 0;
		}
		.fans {
			padding: 23px 0;
		}
		.product h2 {
			margin-top: 23px;
			margin-bottom: 5px;
		}
		.category {
			letter-spacing: -0.5px;
		}
       
	}
	&.type4 {
		.products-top {
			margin-bottom: 13px;
		}
		.btn-load-more {
			margin-top: 80px;
		}
		.grid-item-width {
			width: 25.53191489361702%;
		}
		.grid-item-width2 {
			width: 51.06382978723404%;
		}
		.grid-item-width3 {
			width: 48.93617021276596%;
		}
        .links {
            margin-top: 25px;
        }
		.masonry-grid {
			margin: -2.5px;
		}
		.product {
			padding: 2.5px;
		}
		.products-container {
			padding-top: 26px;
			.masonry-grid {
				margin: -2.5px;
			}
		}
        
	}

	&.type5,
	&.type6 {
		.products-container {
			padding-top: 27px;
		}
		.products-top {
			margin-bottom: 14px;
		}
		.links {
			margin-top: 26px;
		}
	}
	&.type5 {
		.btn-load-more {
			margin-top: 81px;
            margin-bottom: 40px;
		}
        .masonry-grid {
            margin-top: -14px;
        }
        .product {
            padding-bottom: 15px;
            padding-top: 14px;
        }
        .over-comment {
            h3 {
                margin-top: -5px;
            }
        }
	}
	&.type6 {
		.btn-load-more {
			margin-bottom: 40px;
            margin-top: 69px;
		}
        .masonry-grid {
            margin: -13px -11px 0;        
        }
        .product {
            margin: 0 -4px;
            padding: 11px 0;
        }
        .over-comment {
            h3 {
                margin-top: -1px;
            }
        }
	}
}

.category-container {
	position: relative;
}

.filter-container.collapsable {
	background-color: #fff;
	padding-top: 10px;
	padding-bottom: 40px;
	position: absolute;
	top: 15px;
	width: 100%;
	z-index: 99;
	
	&:hover {
		display: block;
	}
	
	& > div {
		border-top: 1px solid #f6f6f6;
		padding-top: 50px;
	}

	.filter-column {
		.title {
			h2 {
				color: $color-light;
				font-family: $font-primary;
				font-size: 13px;
				font-weight: bold;
				letter-spacing: 1.1px;
			}
		}	
		
		.content {
			margin-top: 19px;
			padding-bottom: 0;
			
			ul {
				overflow: auto;

				li {
					margin-top: 10px;
					margin-bottom: 13px;

					label {
						color: $color-light;
						font-family: $font-primary;
						font-size: 11px;
						font-weight: normal;
						margin: 0px 0px 0px 10px;
						overflow: auto;
						padding: 0px;
					}
				}
			}
		}
	}
}


.home3 {
	@media (min-width: 991px) {
		.rev_slider3 {
			margin-left: 270px;
			width: calc(100% - 270px);
			position: relative;
		}
	}
		
	.content-main {
		margin-top: 40px;
		
		.products-top {
			& > div {
				line-height: 36px;
			}
			
			@media (max-width: 768px) {
				ul,
				.show-filter {
					float: none !important;
					text-align: center;
				}
				
				ul li {
					width: 30%;
				}
			}
		}
		
		.product {
			.image-comment {
				padding-bottom: 22px;
			}
			
			.over-comment {
				.rating,
				.price {
					color: $color18;
					font: 12px $font-primary;
					margin-bottom: 10px;
				}
				.rating {
					color: $color22;
					font: 16px $font-secondary;
					margin-left: 20px;
				}
				.price {
					ins {
						color: $color22;
						font: 19px $font-secondary;
						text-decoration: none;
					}
				}
				.links {
					margin-top: 20px;
					
					> * {
						margin-left: 5px;
					}
				}
				
				h2 {
					color: $color28;
				}
				
				@media (min-width: 1440px) and (max-width: 1600px) {
					.links > * {
						margin-left: 3px;
					}
				}
			}
		}
		
		.row.action-field {
			margin-top: 49px;
			
			button {
				letter-spacing: 0;
			}
		}
		
		.btn-filter {
			color: $color24;
			font-family: $font-secondary;
			font-size: 14px;
			font-weight: normal;
		}
		
		.filter-container {
			.filter-column {
				float: left;
				margin-left: 1.25%;
				margin-right: 1.25%;
				width: 12.5%;
				
				ul {
					float: left;
					width: 100%;
				}
				
			}
			
			.filter-brand,
			.filter-color {
				width: 25%;
				
				ul {
					width: 50%;
				}
			}
		
			@media (max-width: 1440px) {
				.filter-column {
					margin-left: 20px;
					margin-right: 0;
					width: auto;
					
					ul {
						width: 160px;
						white-space: nowrap;
					}
				}
			}
		}
	}
}

@media (max-width: 1199px){
    .shopping-cart .resp-vtabs .nav-tabs > li > a {
        font-size: 13px;
    }
}
@media (max-width: 991px){
	.portfolio-item.type4 {
		.grid-item-width {
			width: 33.33333333333333%;
			.over-comment {
				h3,
				.category {
					display: none;
				}
			}
		}
		.grid-item-width2
		,.grid-item-width3 {
			width: 66.66666666666667%;
		}
	}
    .home2 {
        .masonry-grid {
            .grid-width {
                width: 50%;
            }
            .grid-width2 {
                width: 100%;
            }
        }
    }
    .cart-table {
        td .image-comment {
            padding-left: 0 !important;
        }
    }    
}

@media (max-width: 767px) {
	.portfolio-item.type4 {
		.grid-item-width {
			width: 50%;
		}
		.grid-item-width2
		,.grid-item-width3 {
			width: 100%;
			.over-comment {
				h3,
				.category {
					display: none;
				}
			}
		}
	}		
	.home2 .masonry-grid {
        margin: -10px -5px 35px;
		.over-comment .links{
			a {
				display: none;
			}
		}
        .grid-item {
            padding: 10px;
        }
	    .grid-width {
	        width: 50%;
	    }
	    .grid-width2 {
	        width: 100%;
	    }
	}
    .home1 {
        .newsletter,
        .advertisements {
            float: none;
            padding-left: 0;
            width: 100%;
        }
    }

}

