#contactForm {
    .row {
        padding-bottom: 10px;
        padding-top: 10px;
        input,textarea {
            border-radius: 0px;
            border-color: #f3f3f3;
        }
    }
}

.contactus_type1 {
    #contactForm {
        input[type="text"] {
            height: 50px;
        }
        textarea {
            height: 200px;
        }
        input[type="submit"] {
            width: 167px;
            height: 43px;
            background: transparent;
            margin-top: 10px;
        }
    }
}



