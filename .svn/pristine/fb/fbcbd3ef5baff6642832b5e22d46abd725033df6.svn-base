.content-header h4{
    color: #888888;
    font-family: $font-secondary;
    font-size: 20px;
    letter-spacing: 1.6px;
}

h3 {
    color: #777777;
    font-family: $font-secondary;
    font-size: 25px;
    letter-spacing: 3.2px;
}

.contactus.type1 {
    h1,h2,h3,h4,h5,h6 {
        font-family: $font-secondary;
    }
}



.blog.type1 {
    h1,h2,h3,h4,h6 {
        font-family: $font-secondary;
    }
    h1 {
        font-size: 23px;
        letter-spacing: 1.6px;
        margin: 10px 0;
    }
    
    h3 {
        font-size: 23px;
    }
    
    
    h5 {
        font-family: $font-secondary;
        font-size: 14px;
    }
}
.blog.type1,.blog.type2,.blog.type3{
    .sidebar h4 {
        font-size: 16px;
        border-bottom: 1px solid #cccccc;
    }
}