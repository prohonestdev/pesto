			}
			
			#brandwidget {
				padding-bottom: 0;
			}
			
			#colorwidget {
				padding-bottom: 0;
			}
			
			#sizewidget {
				padding-bottom: 0;
			}
			
			#pricewidget {
				color: #999;
				font-family: $font-primary;
				font-size: 14px;
				padding-bottom: 5px;
				
				.btn {
					color: #b2b2b2;
					border: 2px solid #ebebeb;
					font-weight: bold;
					font-size: 12px;
					
				}
				
				.btn-clear {
					letter-spacing: 0px;
					padding: 5px 18px;
				}
				
				.btn-ok {
					padding: 5px 12px;
					letter-spacing: 2px;
				}
				
				input {
					color: #999;
					font-size: 13px;
					padding: 5px 12px;
				}
			}
			
			#bestsellerswidget {
				padding-bottom: 5px;
				
				.popular-post {
					margin-top: -3px;
				}
				
				.bestseller-item {
					margin-top: 30px;
					overflow: auto;
				
					.image {
						float: left;
						width: 37.037037%;
					}
					
					.image-comment {
						display: inline-block;
						margin-left: 18px;
						margin-top: 10px;
						width: 55.4%;
					}
					
					.price-box {
						margin-top: 3px;
					}
					
					p {
						&.title {
							color: #8c8c8c;
							font-family: $font-primary;
							font-size: 14px;
							letter-spacing: -.45px;
							line-height: 1.33;
							white-space: normal;
						}
						
						&.price {
							color: #686868;
							font-family: $font-secondary;
							font-size: 16px;
							white-space: normal;
							
							span.oldprice {
								color: #c1c1c1;
								font-size: 13px;
								letter-spacing: 0.5px;
								text-decoration: line-through;
							}
						}
					}
				}
			}
			
			.panel-group-body {
				margin-top: 18px;
							
				.checkbox-list {
					margin: 0;
					padding: 0;
					
					li {
						padding: 8px 0 .1px;
						
						.checkbox-text {
							margin-left: 5px;
						}
					}
				}
			}
			
			.panel {
				border: 0;
				border-radius: 0;
				
				&>.panel-heading {
					background-color: white;
					margin-left: -5px;
					position: relative;
					
					h5 {
						color: #9e9e9e;
						font-family: $font-primary;
						font-size: 14px;
					}
					
					a.toggle {
						display: block;
						height: $caret-size;
						line-height: $caret-size;
						margin-top: -10px;
						position: absolute;
						right: 2px;
						text-align: center;
						text-decoration: blink;
						top: 50%;
						width: $caret-size;
						
						i {
							line-height: $caret-size;
						}
					}
				}
				
				.panel-body {
					margin-top: 2px;
					padding: 10px 15px;
					
					ul {
						padding-left: 5px;
						
						li {
							padding-bottom: 4.5px;
							padding-top: 4px;
						}
					}
					
					a {
						color: #9e9e9e;
						font-family: $font-primary;
						font-size: 14px; 
					}
				}
			}
		}
	}
}

