//GridLayout

/* ---- isotope ---- */

.grid {
    background: #FFF;
}

/* clear fix */
.grid:after {
    content: '';
    display: block;
    clear: both;
}

.grid-item {
    float: left;
}

.grid-item img {
    display: block;
    height: 100%; 
    width: 100%;
} 

/* ---- .grid-item ---- */
.grid-rectangle-3-columns {
    .grid-sizer,
    .grid-item {
        padding: 15px;
        width: 33.33333333%;
    }
}

.grid-square {
    .grid-sizer,
    .grid-item { 
        padding: 10px;
        width: 25%;
    }
    
    /* 2 columns */
    .grid-item-width2 {
     width: 50%; 
    }

    /* 3 columns */
    .grid-item-width3 {
     width: 75%; 
    }

    /* 4 columns */
    .grid-item-width4 {
     width: 100%; 
    }

    /* 1/2 rows */
    .grid-item-height50 {
        &:before {
            content: '';
            padding-top: 50%;
            display: inline-block;
            float:left;
        }
    }
    
    /* 1 rows */
    .grid-item-height100 {
        &:before {
            content: '';
            float:left;
            padding-top: 100%;
        }
    } 
       
    /* 2 rows */
    .grid-item-height200 {
        &:before {
            content: '';
            display: inline-block;
            float:left;
            padding-top: 200%;
        }
    }

    /* 3 rows */
    .grid-item-height300 {
        &:before {
            content: '';
            display: inline-block;
            float:left;
            padding-top: 300%;
        }
    }
}

.grid-masonry {
    .grid-sizer,
    .grid-item { 
        padding: 2.5px;
        width: 25%;
    }
    
    /* 2 columns */
    .grid-item-width2 {
     width: 50%; 
    }

    /* 3 columns */
    .grid-item-width3 {
     width: 75%; 
    }

    /* 4 columns */
    .grid-item-width4 {
     width: 100%; 
    }

    /* 1/2 rows */
    .grid-item-height50 {
        &:before {
            content: '';
            display: inline-block;
            float:left;
            padding-top: 50%;
        }
    }
    
    /* 1 rows */
    .grid-item-height100 {
        &:before {
            content: '';
            float:left;
            padding-top: 100%;
        }
    } 
       
    /* 2 rows */
    .grid-item-height200 {
        &:before {
            content: '';
            display: inline-block;
            float:left;
            padding-top: 200%;
        }
    }

    /* 3 rows */
    .grid-item-height300 {
        &:before {
            content: '';
            display: inline-block;
            float:left;
            padding-top: 300%;
        }
    }
}


