.content-header {
    margin-bottom: 40px;
    .breadcrumb {
        background: transparent;
        margin: 0;
        li a{
            color: #aaaaaa;
            font-family: $font-primary;
            font-size: 13px;
        }
        li:last-child {
            color: #777777;
            font-family: $font-primary;
            font-size: 13px;
        }
    }
    .container {
        border-bottom: 1px solid #f2f2f2;
        border-top: 1px solid #f2f2f2;
        padding-bottom: 16px;
        padding-top: 16px;
    }
}

