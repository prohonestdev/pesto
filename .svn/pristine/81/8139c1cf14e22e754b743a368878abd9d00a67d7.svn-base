@font-face {
  font-family: 'pesto';
  src: url('../font/pesto.eot?56340086');
  src: url('../font/pesto.eot?56340086#iefix') format('embedded-opentype'),
       url('../font/pesto.woff?56340086') format('woff'),
       url('../font/pesto.ttf?56340086') format('truetype'),
       url('../font/pesto.svg?56340086#pesto') format('svg');
  font-weight: normal;
  font-style: normal;
}
